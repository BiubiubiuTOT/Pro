<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_unit_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="单位名称"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="地址"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_level"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="资质等级"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_certificate_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="资质证书号"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_legal_person"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="法人代表"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_legal_person_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="联系电话"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_legal_person_idcard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="身份证号码"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_proj_manager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="项目负责人"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_proj_manager_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="联系电话"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_proj_manager_idcard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="身份证号码"
        app:item_title_size="14sp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_org_code"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_content_size="14sp"
        app:item_right_arrow="false"
        app:item_title="企业组织机构代码"
        app:item_title_size="14sp" />
</LinearLayout>