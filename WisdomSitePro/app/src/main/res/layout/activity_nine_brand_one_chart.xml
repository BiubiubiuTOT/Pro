<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context=".ui.contacts.NineBrandOneChartActivity">

    <include layout="@layout/layout_title_white" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_project_overview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="工程概况牌" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_manager_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="管理人员名单及监督电话牌" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_fire_protection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="消防保卫牌" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_safe_product"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="安全生产牌" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_civilized_construction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="文明施工牌" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_site_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="施工现场平面布置图" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_migrant_workers"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="农民工维权告示牌" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_project_progress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="工程形象进度" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_five_part"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_right_arrow="false"
        app:item_title="五方责任主体" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.ItemView
        android:id="@+id/item_project_design_sketch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:item_bottom_line="false"
        app:item_right_arrow="false"
        app:item_title="工程效果图" />
</LinearLayout>