<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.InputItemView
        android:id="@+id/input_certificate_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:inputItemHint="请选择"
        app:inputItemRightArrow="true"
        app:inputItemTitle="证书名称" />

    <com.ljkj.qxn.wisdomsitepro.Utils.widget.InputItemView
        android:id="@+id/input_certificate_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:inputItemTitle="证书编号" />
</LinearLayout>